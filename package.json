{
  "name": "appium-template-plugin",
  "version": "1.0.0",
  "description": "",
  "main": "./lib/index.js",
  "scripts": {
    "build": "npx tsc",
    "test": "mocha --require ts-node/register test/plugin.spec.js --timeout 60000 --exit",
    "prepublish": "npx tsc",
    "lint": "eslint '**/*.js' --fix",
    "prettier": "prettier '**/*.js' --write --single-quote",
    "uninstall": "(appium plugin uninstall element-wait || exit 0)",
    "install-plugin": "npm run build && appium plugin install --source=local $(pwd)",
    "reinstall-plugin": "(appium plugin uninstall template-plugin || exit 0) && npm run install-plugin"
  },
  "contributors": [
    {
      "name": "Saikrishna",
      "email": "saikrishna321@yahoo.com"
    },
    {
      "name": "Srinivasan Sekar",
      "email": "srinivasan.sekar1990@gmail.com"
    }
  ],
  "license": "ISC",
  "peerDependencies": {
    "appium": "^2.0.0-beta.57"
  },
  "dependencies": {
    "@appium/base-plugin": "^2.2.1",
    "@appium/support": "^3.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@types/chai": "^4.2.16",
    "@types/lodash": "^4.14.170",
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.6.1",
    "appium-fake-driver": "^2.2.0",
    "appium-uiautomator2-driver": "^2.12.6",
    "babel-eslint": "^10.0.1",
    "chai": "^4.1.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "mocha": "^8.3.2",
    "prettier": "^2.0.5",
    "sinon": "^11.0.0",
    "ts-node": "^10.9.1",
    "typescript": "4.7.4",
    "webdriverio": "7.20.9"
  },
  "appium": {
    "pluginName": "template-plugin",
    "mainClass": "TemplatePlugin",
    "schema": {
      "$schema": "http://json-schema.org/draft-07/schema",
      "additionalProperties": false,
      "description": "Appium configuration schema for the template plugin.",
      "properties": {
        "interval": {
          "type": "number"
        }
      },
      "title": "Appium template plugin",
      "type": "object"
    }
  },
  "engines": {
    "node": ">=10",
    "npm": ">=6"
  },
  "lint-staged": {
    "src/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "npm run prettier",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test"
    }
  },
  "files": [
    "lib"
  ]
}
